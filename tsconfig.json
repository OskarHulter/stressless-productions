{
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"jsx": "preserve",
		"sourceMap": false,
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"strict": true,
		"strictNullChecks": true,
	},
	"exclude": [
		"out",
		".next"
	]
}